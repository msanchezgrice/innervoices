{
  "framework": "vite",
  "installCommand": "cd innervoices && npm install",
  "buildCommand": "cd innervoices && npm run build && rm -rf ../dist && mv dist ../dist",
  "devCommand": "cd innervoices && npm run dev",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
